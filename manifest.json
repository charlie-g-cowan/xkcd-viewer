{
  "manifest_version": 2,

  "name": "xkcd viewer",
  "description": "Chrome extension that can display and search for xkcd webcomics",
  "version": "1.0",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" 
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://xkcd.com/"
  ]
}
